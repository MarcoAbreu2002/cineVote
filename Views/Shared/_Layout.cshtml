


<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - cineVote</title>
    <link rel="icon" type="image/png" href="data:image/png;base64," />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/cineVote.styles.css" asp-append-version="true" />
</head>

<style>
    .margin-right{
    margin-right: 2%;
    }

    .margin-left{
    margin-left: 2%
    }
</style>


<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm border-bottom box-shadow mb-3">
            <div class="container-fluid margin-left">
                <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">CineVote</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse"
                        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <ul class="navbar-nav margin-right">
                    @if (string.IsNullOrEmpty(User.Identity.Name))
                            {
                        <li class="nav-item ">
                            <a class="nav-link text-white" asp-area="" asp-controller="UserAuthentication" asp-action="Registration">Create Account</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="UserAuthentication" asp-action="Login">Login</a>
                        </li>
                    }
                    @if(User.IsInRole("admin"))
                    {
                        <li class="nav-item">
                            <a class="nav-link text-white" href="/Admin/AdminDashboard">Admin Dashboard</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link text-white" href="/Competition/DisplayCompetition">Competitions</a>
                        </li>
                    }
                    @if (User.IsInRole("user"))
                    {
                        <li class="nav-item ">
                            <a class="nav-link text-white" href="/Competition/DisplayCompetition">Competitions</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link text-white" href="/User/Profile?username=@User.Identity.Name">Profile</a>
                        </li>
                    }
                    @if (!string.IsNullOrEmpty(User.Identity.Name))
                    {
                        <li><a class="nav-link text-white" asp-area="" asp-controller="UserAuthentication" asp-action="Logout">Logout</a></li>
                    }

                </ul>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - cineVote 
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>